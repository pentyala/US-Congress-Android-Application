<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_comm_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.pentyala.hwfinal.CommDetails">
    <ImageView
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:src="@drawable/star"
        android:id="@+id/imageView9"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="10dp"
        />
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/imageView9"
        android:layout_centerHorizontal="true">
        <TextView
            android:layout_row="0"
            android:layout_column="0"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:text="Committee ID:"
            android:paddingBottom="10dp"
        />
        <TextView
            android:id="@+id/textView21"
            android:layout_row="0"
            android:textAllCaps="true"
            android:gravity="right"
            android:layout_width="200dp"
            android:text="Text View"
            android:layout_column="2"
            android:paddingBottom="10dp"
        />

        <TextView
            android:layout_row="1"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:layout_column="0"
            android:text="Name:"
            android:paddingBottom="10dp"
            />
        <TextView
            android:id="@+id/textView22"
            android:layout_row="1"
            android:gravity="right"
            android:layout_width="200dp"            android:text="Text View"
            android:paddingBottom="10dp"
            android:layout_column="2"
            />

        <TextView
        android:layout_row="2"
        android:layout_column="0"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
        android:text="Chamber:"
            android:paddingBottom="10dp"
        />

        <TextView
            android:id="@+id/textView23"
            android:layout_row="2"
            android:gravity="right"
            android:layout_width="200dp"            android:text="Text View"
            android:layout_column="2"
            android:paddingBottom="10dp"
            />

        <TextView
        android:layout_row="3"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
        android:layout_column="0"
        android:text="Parent Committee:"
            android:paddingBottom="10dp"
        />
        <TextView
            android:id="@+id/textView24"
            android:layout_row="3"
            android:gravity="right"
            android:layout_width="200dp"
            android:textAllCaps="true"
            android:text="Text View"
            android:paddingBottom="10dp"
            android:layout_column="2"
            />

        <TextView
        android:layout_row="4"
        android:layout_column="0"
            android:paddingBottom="10dp"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
        android:text="Contact:"
        />
        <TextView
            android:id="@+id/textView25"
            android:layout_row="4"
            android:gravity="right"
            android:layout_width="200dp"            android:paddingBottom="10dp"
            android:text="Text View"
            android:layout_column="2"
            />

        <TextView
        android:layout_row="5"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:paddingBottom="10dp"
        android:layout_column="0"
        android:text="Office:"
        />
        <TextView
            android:id="@+id/textView26"
            android:layout_row="5"
            android:paddingBottom="10dp"
            android:gravity="right"
            android:layout_width="200dp"            android:text="Text View"
            android:layout_column="2"
            />
    </GridLayout>




</RelativeLayout>
